import argparse


def main():
    parser = argparse.ArgumentParser()
    parser.add_argument(
        '--version',
        action='version',
        version='Ansible community version {{ version }}',
        help="show the version of the Ansible community package",
    )
    args = parser.parse_args()


if __name__ == '__main__':
    main()
